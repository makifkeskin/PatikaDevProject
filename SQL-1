Select title, description from film;

Select * from film where film_uzunluk > '60' and film_uzunluk < '75';

Select * from film where rental_rate = '0,99' and (replacement_cost = '12.99' or replacement_cost = '28.99');

Select first_name, last_name from customer where name='Mary';

Select * from film where uzunluk < '50' and (rental_rate != '2.99' or rental_rate != '4.99');
